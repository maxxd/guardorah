{
  "manifest_version": 3,
  "name": "Guardorah",
  "version": "1.0",
  "description": "Prevents accidental click-to-edit. Hold Option/Alt key and click-to-edit",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://*.atlassian.net/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
